{
  "name": "profile",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "tsx --watch  --env-file .env.local server.ts",
    "dev:staging": "tsx --watch --env-file .env.staging server.ts",
    "dev:prod": "tsx --watch --env-file .env.production server.ts",
    "start": "node --env-file .env.production dist/server.js",
    "build": "tsc && tsc-alias",
    "type-check": "tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "prepare": "husky",
    "test": "vitest",
    "test:run": "vitest --run",
    "test:ui": "vitest --ui",
    "coverage": "vitest run --coverage"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "axios": "^1.12.2",
    "basic-auth": "^2.0.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-validator": "^7.2.1",
    "express-xss-sanitizer": "^2.0.1",
    "helmet": "^8.1.0",
    "morgan": "^1.10.1",
    "winston": "^3.18.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.38.0",
    "@tsconfig/node22": "^22.0.2",
    "@types/cookie-parser": "^1.4.9",
    "@types/cors": "^2.8.19",
    "@types/eslint": "^9.6.1",
    "@types/express": "^5.0.3",
    "@types/express-xss-sanitizer": "^2.0.0",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.8.1",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/eslint-plugin": "^1.3.23",
    "eslint": "^9.38.0",
    "eslint-plugin-perfectionist": "^4.15.1",
    "husky": "^9.1.7",
    "jiti": "^2.6.1",
    "lint-staged": "^16.2.4",
    "prettier": "^3.6.2",
    "tsc-alias": "^1.8.16",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.46.1",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint",
      "prettier --check"
    ],
    "*.{json,yml,yaml,md}": [
      "prettier --check"
    ]
  }
}
